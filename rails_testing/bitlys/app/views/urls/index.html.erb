<h1>Listado Urls</h1>

<%= link_to 'New Url', new_url_path %>
<table>
	<tr>
		<th>Url Original</th>
		<th>Short Url</th>
		<th>Counter</th>
	</tr>
	<% @urls.each do |url| %>
		<tr>
			<td><%=url.original %></td>
			<td><%=link_to "#{url.short_url}" edit_url_path(url) %></td>
			<td><%=url.click_count %></td>
		</tr>
	<% end %>
</table>










