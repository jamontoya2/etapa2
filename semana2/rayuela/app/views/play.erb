
<h1 id="main-title">Â¡RAYUELA!</h1>
    <button id="start_btn">Jugar</button><br><br>

  <h1 id="count"></h1>
  <h1 id="result"></h1>
  <div class="game_id" id="<%=@game.id%>"></div>
    <table class="table">
      <tr id="Player1">
        <td id="player1"><strong>Jugador:1</strong></td>
        <td id="p1_1" class="active"></td>
        <%i = 2%>
        <%99.times do%>
          <%if i == 90 %>
          <td id="p1_<%=i%>"></td>
          <%else%>
          <td id="p1_<%=i%>"></td>
          <%end%>
        <%i += 1%>
        <%end%>
      </tr>

      <tr id="Player2">
        <td id="player2"><strong>Jugador:2</strong></td>
        <td id="p2_1" class="active"></td>
        <%i = 2%>
        <%99.times do%>
        <%if i == 90 %>
          <td id="p2_<%=i%>" class="finish"></td>
          <%else%>
          <td id="p2_<%=i%>"></td>
          <%end%>
        <%i += 1%>
        <%end%>
      </tr>
</table>
 

<div id="form_stats"></div>
